    "panel.border": "#1e5271",
    "panelTitle.activeBorder": "#cbd6e2", // underliner
    "panelTitle.activeForeground": "#ffffff",
    "panelTitle.inactiveForeground": "#99acc2",

    // Terminal
    "terminal.selectionBackground": "#516f9099",
    "terminal.background": "#1e5271",
    "terminal.foreground": "#eaf0f6",
    "terminalCursor.background": "#253342",
    "terminalCursor.foreground": "#99acc2",
    "terminal.ansiBlack": "#7c98b6",
    "terminal.ansiRed": "#f2545b",
    // "terminal.ansiGreen": "#25c082",
    "terminal.ansiGreen": "#ff4c00", // zCoder@-
    "terminal.ansiYellow": "#f5c26b",
    // "terminal.ansiYellow": "#9d8d6c",
    "terminal.ansiBlue": "#00a4bd",
    "terminal.ansiMagenta": "#89DDFF",
    "terminal.ansiCyan": "#ff8f59",
    "terminal.ansiWhite": "#ffffff",
    "terminal.ansiBrightBlack": "#7c98b6",
    "terminal.ansiBrightRed": "#f2545b",
    "terminal.ansiBrightGreen": "#04c87a",
    // "terminal.ansiBrightYellow": "#f5c26b",
    "terminal.ansiBrightYellow": "#9d8d6c",
    "terminal.ansiBrightBlue": "#00a4bd",
    "terminal.ansiBrightMagenta": "#89DDFF",
    "terminal.ansiBrightCyan": "#ff8f59",
    "terminal.ansiBrightWhite": "#ffffff",

    // Barra Menu desplegable (Archivo, editar, selección, ver, ir, ejecutar, terminal, ayuda)
    "menu.background": "#1e5271",
    "menu.foreground": "#cbd6e2",
    "menu.selectionForeground": "#ffffff",
    "menu.selectionBackground": "#27607c",
    "menu.separatorBackground": "#99acc2",

    // contrast
    "contrastBorder": "#ffffff00",

    // dropdown
    "dropdown.background": "#1e5271",
    "dropdown.border": "#15232d",
    "dropdown.foreground": "#fff",

    // selected Text colours
    // This is the standard Select colour
    // "editor.selectionBackground": "#476b91", //v0
    // "editor.selectionBackground": "#1e5271", //v1
    "editor.selectionBackground": "#992630CE",

    // This is the colour of the other matching elements
    "editor.selectionHighlightBackground": "#0050A480",

    // Scrollbar
    "scrollbarSlider.activeBackground": "#cbd6e2cc",
    "scrollbarSlider.background": "#1e5271",
    "scrollbarSlider.hoverBackground": "#cbd6e266",

    // Badges
    "badge.background": "#cbd6e2",
    "badge.foreground": "#1e5271"
