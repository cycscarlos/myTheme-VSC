    // Canvas colors
    "editor.background": "#266b8d",
    "editor.foreground": "#ffffff",

    // Groups/Tabs
    "editorGroup.border": "#51122738",
    "editorGroup.dropBackground": "#516f9099",
    "editorGroupHeader.noTabsBackground": "#27607c",
    "editorGroupHeader.tabsBackground": "#27607c",
    "editorGroup.emptyBackground": "#27607c",
    "editorGroup.focusedEmptyBorder": "#7c98b6",

    // Activity Bar
    "activityBar.background": "#1e5271",
    "activityBar.foreground": "#ffffff", // 1er icono
    "activityBar.border": "#27607c",
    "activityBar.inactiveForeground": "#99acc2", // resto de iconos
    "activityBarBadge.background": "#f2547d",
    "activityBarBadge.foreground": "#ffffff",

    // Sidebar
    "sideBar.background": "#27607c",
