    "sideBar.dropBackground": "#27607c",
    "sideBarTitle.foreground": "#ffffff",
    "sideBarSectionHeader.background": "#27607c",
    "sideBarSectionHeader.foreground": "#ffffff",
    "sideBarSectionHeader.border": "#27607c",

    // Title Bar (top)
    "titleBar.activeBackground": "#1e5271",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#27607c",
    "titleBar.inactiveForeground": "#99acc2",

    // Status Bar (bottom)
    "statusBar.background": "#1e5271",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingBackground": "#f2545b",
    "statusBar.noFolderBackground": "#6a78d1",
    "statusBarItem.prominentBackground": "#0091ae",
    "statusBarItem.prominentHoverBackground": "#007a8c",

    // Breadcrumbs
    "breadcrumb.focusForeground": "#ffffff",
    "breadcrumb.activeSelectionForeground": "#7fd1de",

    // Panel (área del terminal integrado)
